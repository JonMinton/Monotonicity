fn <- function(x, n_dp=3){
mu <- x %>%
mean %>%
round(n_dp)
n_obs <- length(x)
upper_975 <- mean(x) + 1.96 * sd(x)/sqrt(n_obs)
se=((upper_975 - mean(x))/1.96) %>%
round(n_dp)
out <- list(mu=mu, se=se)
return(out)
}
