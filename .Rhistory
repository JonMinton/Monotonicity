#Bootstrap function
bootstrap_data <- function(x, n_rep = 1000){
k <- ncol(x)
N <- nrow(x)
out_df <- matrix(NA, nrow = n_rep, ncol = k)
for (i in seq_along(1:n_rep)){
rows_to_sample <- sample(seq_along(1:N), replace = T)
y <- x[rows_to_sample,]
out <- sapply(X = y, mean)
out_df[i,] <- out
}
colnames(out_df) <- colnames(x)
out_df %>% data.frame %>% tbl_df
}
